<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dome2">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Cdk"/>
        <activity android:name=".Toy"/>
        <activity android:name=".Skin"/>
        <activity android:name=".Player"/>
        <activity android:name=".CdkActivity"/>
        <activity android:name=".PlayerActivity"/>
        <activity android:name=".OrderActivity"/>
        <activity android:name=".SaleActivity"/>
        <activity android:name=".PifuActivity"/>
        <activity android:name=".CdkLists"/>
        <provider
            android:authorities="666"
            android:name="com.example.dome2.Provider"
            android:exported="true"
            />
    </application>


</manifest>